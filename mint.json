{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Nuplace",
  "logo": {
    "dark": "/logo/dark.svg",
    "light": "/logo/light.svg",
    "href": "https://docs.nuplace.io"
  },
  "favicon": "/favicon.svg",
  "colors": {
    "primary": "#9159d6",
    "light": "#b584f1",
    "dark": "#9159d6",
    "anchors": {
      "from": "#9159d6",
      "to": "#b584f1"
    }
  },
  "topAnchor": {
    "name": "topAnchor",
    "icon": "book-open",
    "iconType": "solid"
  },

  "topbarLinks": [
    {
      "name": "Demo",
      "url": "https://meet.nuplace.io/ryabar"
    }
  ],
  "topbarCtaButton": {
    "name": "Empieza",
    "url": "https://dashboard.nuplace.io"
  },
  "tabs": [
    {
      "name": "API Referencia",
      "url": "api-reference"
    },
    {
      "name": "Integraciones",
      "url": "integrations"
    }
  ],
  "modeToggle": {
    "default": "dark",
    "isHidden": false
  },

  "anchors": [
    {
      "name": "DocumentationX",
      "icon": "book-open-cover",
      "url": "https://mintlify.com/docs"
    },
    {
      "name": "Community",
      "icon": "slack",
      "url": "https://mintlify.com/community"
    },
    {
      "name": "Blog",
      "icon": "newspaper",
      "url": "https://mintlify.com/blog"
    },
    {
      "name": "API Reference V1",
      "url": "/v1/api-reference",
      "version": "v1"
    },
    {
      "name": "API Reference V2",
      "url": "/v2/api-reference",
      "version": "v2"
    },
    {
      "name": "Anchor Without a Version",
      "url": "example-anchor"
    }
  ],
  "navigation": [
    {
      "group": "Get Started",
      "pages": ["introduction", "quickstart", "development"]
    },
    {
      "group": "Essentials",
      "pages": [
        "essentials/markdown",
        "essentials/code",
        "essentials/images",
        "essentials/settings",
        "essentials/navigation"
      ]
    },
    {
      "group": "API Documentation",
      "pages": ["api-reference/introduction"]
    },
    {
      "group": "Endpoint Examples",
      "pages": [
        "api-reference/endpoint/get",
        "api-reference/endpoint/create",
        "api-reference/endpoint/delete",
        "api-reference/endpoint/patch"
      ]
    }
  ],
  "footerSocials": {
    "twitter": "https://twitter.com/mintlify",
    "github": "https://github.com/mintlify",
    "linkedin": "https://www.linkedin.com/company/mintsearch"
  }
}
